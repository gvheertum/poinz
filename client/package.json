{
  "name": "poinz-client",
  "version": "0.13.3",
  "description": "Distributed Planning Poker",
  "private": true,
  "author": "xeronimus@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/Zuehlke/poinz",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zuehlke/poinz.git"
  },
  "scripts": {
    "start": "npm run serve",
    "lint": "eslint \"app/**/*.js\" \"test/**/*.js\" \"cypress/**/*.js\"",
    "format": "prettier --write \"app/**/*.js\" \"cypress/**/*.js\" \"test/**/*.js\" \"./*.js\"",
    "test": "jest",
    "test:e2e": "npm run cypress:run",
    "pretest": "npm run lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "serve": "cross-env BABEL_ENV=development webpack serve --config webpack.config.js --progress",
    "build": "cross-env BABEL_ENV=production webpack --progress --bail --json --config  webpack.production.config.js > webpack-build-stats.json",
    "build:analyze": "cross-env BABEL_ENV=production webpack --progress --colors --bail --config  webpack.analysis.config.js"
  },
  "jest": {
    "testMatch": [
      "**/test/unit/**/*Test.js",
      "**/test/integration/**/*Test.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/unit/setup.js"
    ]
  },
  "browserslist": [
    "defaults",
    "edge 18"
  ],
  "dependencies": {
    "axios": "0.24.0",
    "history": "5.1.0",
    "loglevel": "1.8.0",
    "node-polyglot": "2.4.2",
    "purecss": "2.0.6",
    "react": "17.0.2",
    "react-anchorify-text": "2.4.1",
    "react-dom": "17.0.2",
    "react-dropzone": "11.4.2",
    "react-redux": "7.2.6",
    "react-test-renderer": "17.0.2",
    "redux": "4.1.2",
    "redux-thunk": "2.4.0",
    "reselect": "4.1.4",
    "socket.io-client": "4.4.0",
    "styled-components": "5.3.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "1.0.1",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.3",
    "babel-plugin-styled-components": "1.13.3",
    "cross-env": "7.0.3",
    "css-loader": "6.5.1",
    "cypress": "9.0.0",
    "date-fns": "2.26.0",
    "eslint": "8.3.0",
    "eslint-plugin-react": "7.27.1",
    "file-loader": "6.2.0",
    "jest": "27.3.1",
    "style-loader": "3.3.1",
    "url-loader": "4.1.1",
    "webpack": "5.64.2",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.5.0"
  },
  "engines": {
    "node": ">=14"
  }
}
